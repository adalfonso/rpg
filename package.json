{
  "name": "rpg",
  "version": "0.51.5",
  "description": "An RPG of some sort... we'll see.",
  "scripts": {
    "start": "npm run dev",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "npm compile && electron-builder",
    "dist:dir": "npm dist --dir -c.compression=store -c.mac.identity=null",
    "test": "mochapack --colors -r jsdom-global/register --webpack-config tests/webpack.config.js tests/**/*.test.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "lint": "npx eslint --fix --ext .ts src/"
  },
  "author": "Anthony Dalfonso <anthonyjdalfonso@gmail.com>",
  "license": "BSD",
  "dependencies": {
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.20.42",
    "@types/sinon": "^9.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chai": "^4.2.0",
    "electron": "^16.0.0",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.3",
    "mochapack": "^1.1.15",
    "node-sass": "^6.0.1",
    "nyc": "^14.1.1",
    "sass-loader": "^10.2.1",
    "sinon": "^9.2.4",
    "tiled-types": "^1.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.10",
    "webpack": "~4.42.1"
  }
